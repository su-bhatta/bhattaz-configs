use_xft yes
xftfont Monospace:pixelsize=12
#xftalpha 0.9
update_interval 2
total_run_times 0
own_window yes
own_window_type desktop
own_window_transparent yes
own_window_hints undecorated,below,sticky,skip_taskbar,skip_pager
own_window_colour 000000
own_window_argb_visual yes
own_window_argb_value 0
short_units yes
format_human_readable yes
double_buffer yes
draw_shades no
draw_outline no
draw_borders no
draw_graph_borders no
default_color BFBFBF
default_shade_color 263041
default_outline_color 263041
alignment top_left
gap_x 0
gap_y 0
no_buffers yes
uppercase no
cpu_avg_samples 1
net_avg_samples 1
override_utf8_locale yes
use_spacer none
default_color BDB5A1
default_shade_color 1d1d1d
color0 ffffff
color1 ffffff
color2 D6713C

